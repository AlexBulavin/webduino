{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "versionString",
	"app": {
		"background": {
		  "scripts": [
			"background.js"
		  ]
		}
	},
	"file_handlers": {
	  "text": {
		"types": [
		  "text/*"
		],
	  },
	  "image": {
		"types": [
		  "image/png",
		  "image/jpeg"
		],
		"extensions": [
		  "tiff"
		],
	  },
	  "any": {
		"extensions": [
		  "*",
		  "include_directories": true
		],
	  }
	},
	"offline_enabled": true,
	"permissions": [
	  "tabs",
	  "alarms",
	  "clipboardRead",
	  "clipboardWrite",
	  "contextMenus",
	  "downloads",
	  "geolocation",
	  "notifications",
	  "storage",
	  "system.storage",
	  "tts",
	  "ttsEngine",
	  "unlimitedStorage"
	],
	"optional_permissions": [
	  "unlimitedStorage"
	]
}
